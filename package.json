{
  "name": "media-prototype-2",
  "version": "1.0.0",
  "description": "TypeScript Express.js server with strict type checking",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "dev:type-check": "concurrently \"tsc --noEmit --watch\" \"tsx watch src/index.ts\"",
    "build": "tsc --project tsconfig.build.json",
    "start": "node dist/index.js",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "test:watch": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint src/**/*.ts --fix",
    "lint:check": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "validate": "npm run type-check && npm run lint:check && npm run format:check && npm run test"
  },
  "keywords": ["express", "typescript", "nodejs", "api", "strict-types"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.15.0",
    "jest": "^30.2.0",
    "prettier": "^3.3.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.19.1",
    "typescript": "^5.9.3"
  }
}
