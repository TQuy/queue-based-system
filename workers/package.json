{
  "name": "media-worker",
  "version": "1.0.0",
  "description": "TypeScript Express.js server with strict type checking",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production npm start",
    "type-check": "tsc --noEmit",
    "test": "NODE_ENV=test NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "lint:check": "npm run lint",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "swagger:generate": "tsx scripts/swagger-generate.ts",
    "swagger:dev": "NODE_ENV=development npm run swagger:generate && npm run dev",
    "validate": "npm run type-check && npm run lint:check && npm run format:check && npm run test"
  },
  "dependencies": {
    "@types/morgan": "^1.9.10",
    "amqplib": "^0.10.9",
    "dotenv": "^17.2.3",
    "express": "^5.0.1",
    "http-status-codes": "^2.3.0",
    "morgan": "^1.10.1",
    "redis": "^5.9.0",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.19.2",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "jest-util": "^30.2.0",
    "prettier": "^3.3.3",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "swagger-autogen": "^2.23.7",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3"
  }
}
